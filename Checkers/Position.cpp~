#include <iostream>
#include "Position.h"

Position:: Position(int _x, int _y) {
  x = _x;
  y = _y;
}

Position:: ~Position() {

}

int Position:: getX() {
  return x;
}

int Position:: getY() {
  return y;
}

void Position:: setX(int _x) {
  x = _x;
}

void Position:: setY(int _y) {
  y = _y;
}

bool Position:: operator==(Position& pos) {
  return (pos.x == x && pos.y == y);
}
